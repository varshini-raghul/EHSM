{
    "_version": "1.60.0",
    "sap.app": {
        "id": "com.ehsm.ehsmportal",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.0.2"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "@sap/generator-fiori:basic",
            "version": "1.20.0",
            "toolsId": "c31f747e-afba-474f-a2e8-6a395000dd1e"
        },
        "dataSources": {
            "mainService": {
                "uri": "/sap/opu/odata/sap/ZODATA_EHSM_789_SRV/",
                "type": "OData",
                "settings": {
                    "annotations": [],
                    "localUri": "localService/mainService/metadata.xml",
                    "odataVersion": "2.0"
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": true,
        "dependencies": {
            "minUI5Version": "1.120.14",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {},
                "sap.ui.layout": {},
                "sap.f": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "com.ehsm.ehsmportal.i18n.i18n",
                    "supportedLocales": [
                        "",
                        "en"
                    ],
                    "fallbackLocale": "en"
                }
            },
            "": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {}
            },
            "app": {
                "type": "sap.ui.model.json.JSONModel",
                "uri": "model/AppModel.json"
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "viewPath": "com.ehsm.ehsmportal.view",
                "controlId": "appContainer",
                "controlAggregation": "pages",
                "async": true
            },
            "routes": [
                {
                    "pattern": "",
                    "name": "Login",
                    "target": "Login"
                },
                {
                    "pattern": "Dashboard",
                    "name": "Dashboard",
                    "target": "Dashboard"
                },
                {
                    "pattern": "Profile",
                    "name": "Profile",
                    "target": "Profile"
                },
                {
                    "pattern": "IncidentDetails",
                    "name": "IncidentDetails",
                    "target": "IncidentDetails"
                },
                {
                    "pattern": "RiskDetails",
                    "name": "RiskDetails",
                    "target": "RiskDetails"
                }
            ],
            "targets": {
                "Login": {
                    "viewName": "Login",
                    "viewLevel": 1
                },
                "Dashboard": {
                    "viewName": "Dashboard",
                    "viewLevel": 2
                },
                "Profile": {
                    "viewName": "Profile",
                    "viewLevel": 3
                },
                "IncidentDetails": {
                    "viewName": "IncidentDetails",
                    "viewLevel": 3
                },
                "RiskDetails": {
                    "viewName": "RiskDetails",
                    "viewLevel": 3
                }
            }
        },
        "rootView": {
            "viewName": "com.ehsm.ehsmportal.view.App",
            "type": "XML",
            "id": "App",
            "async": true
        }
    }
}